(jarvLogging) {
  log {
    output file /var/log/caddy/{args.0}.access.log {
        roll_size 100mb
        roll_keep 20
        roll_keep_for 720h
    }
  }
}

jarv.org {
  handle {
          root * /var/opt/www/jarv.org
          file_server {
            precompressed gzip
          }
  }
  import jarvLogging jarv.org
}
